{
  "select": [
    "t1.id as id",
    "t1.name",
    "t1.age" ,
    "t1.email",
    "t2.product_name",
    "t3.total_amount"
  ],
  "from": {
    "t_name": "tbl1",
    "as": "t1"
  },
  "join": [
    {
      "t_name": "tbl2",
      "as": "t2",
      "on": "t1.product_id=t2.product_id"
    },
    {
      "t_name": "tbl3",
      "as": "t3",
      "on": "t1.name=t3.customer_name"
    }
  ],
  "where": "t3.order_id='O002'"
}
